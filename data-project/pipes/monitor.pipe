TOKEN "monitor_read" READ

NODE endpoint
SQL >

    %
    SELECT * FROM log_mv
    WHERE 1 = 1
     {% if defined(sensor_id) %}               
        AND id = {{ Int32(sensor_id)}}
      {% end %}    
    ORDER BY timestamp DESC
    LIMIT {{ Int32(max_results, 100, description="The maximum number of most recent anomalies to return per response.") }}


